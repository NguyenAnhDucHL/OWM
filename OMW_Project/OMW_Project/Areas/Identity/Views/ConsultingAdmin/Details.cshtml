@model OMW_Project.Models.Consulting

@{
    ViewBag.Title = "Details";
}

<h2>Chi tiết lịch tư vấn</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Tên Bác sĩ
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Doctor.FullName)
        </dd>

        <dt>
            Tên bệnh nhân
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Patient.FullName)
        </dd>

        <dt>
            Vấn đề bệnh nhân
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PatientIssue)
        </dd>

        <dt>
            Thời gian tư vấn
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StartConsulting)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Status)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Status)
        </dd>

    </dl>
</div>
<div class="row">
    <div class="col-md-offset-1 col-md-10">
        <a class="btn btn-info" href="@Url.Action("Edit", "ConsultingAdmin", new { id = Model.ConsultingId })">Sửa</a>
        <a class="btn btn-default" href="@Url.Action("Index", "ConsultingAdmin")">Quay lại</a>
    </div>
</div>

