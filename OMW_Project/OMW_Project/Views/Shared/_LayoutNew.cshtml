<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Đông Phương Y Học</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="/Assets/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="https://fonts.googleapis.com/css?family=Raleway:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Lora:400,400i,700,700i" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="/Assets/css/business-casual.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Dancing+Script&display=swap" rel="stylesheet">
</head>
<body>
    <h1 class="site-heading text-center text-white d-none d-lg-block">
        <span class="site-heading-upper text-primary mb-3">Sống Thời Vận, Chết Số</span>
        <span class="site-heading-lower">Đông Phương Y Học</span>
    </h1>
    @Html.Partial("_Header")
    @RenderSection("NavBar", required: false)


    @RenderBody()

    <footer class="footer text-faded text-center py-5">
        <div class="container">
            <p class="m-0 small">Copyright &copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </div>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    <script src="~/Assets/vendor/bootstrap/js/popper.min.js"></script>

    @Scripts.Render("~/bundles/bootstrap")
    <script src="/Assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
